{
  "name": "@bingwu/iip-ui-utils",
  "version": "1.2.14",
  "description": "iip-ui-vue3 工具函数库",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "test": "vitest",
    "test:verbose": "vitest --reporter=verbose",
    "test:coverage": "vitest --coverage",
    "test:run": "vitest run",
    "test:run:coverage": "vitest run --coverage",
    "test:run:verbose": "vitest run --reporter=verbose",
    "clean": "rimraf dist",
    "prepublishOnly": "pnpm build"
  },
  "devDependencies": {
    "tsup": "^8.5.1",
    "typescript": "^5.3.2"
  },
  "keywords": [
    "utils",
    "typescript",
    "vue3",
    "theme",
    "validate",
    "tools"
  ],
  "author": "iip-ui-vue3 Team",
  "license": "MIT",
  "homepage": "https://github.com/BINGWU2003/iip-ui-vue3#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/BINGWU2003/iip-ui-vue3.git",
    "directory": "packages/utils"
  },
  "bugs": {
    "url": "https://github.com/BINGWU2003/iip-ui-vue3/issues"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
