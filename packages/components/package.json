  "dependencies": {
    "@bingwu/iip-ui-utils": "workspace:*",
    "@element-plus/icons-vue": "^2.1.0",
    "dayjs": "^1.11.13"
  },
  "peerDependencies": {
    "element-plus": "^2.11.2",
    "vue": "^3.3.8",
    "vxe-pc-ui": "~4.8.15",
    "vxe-table": "^4.15.6",
    "xe-utils": "^3.7.8"
  },
  "devDependencies": {
    "@element-plus/icons-vue": "^2.1.0",
    "@rollup/plugin-strip": "^3.0.4",
    "@types/node": "^22.16.5",
    "@vitejs/plugin-vue": "^4.5.0",
    "element-plus": "^2.11.2",
    "jsdom": "^23.0.1",
    "sass": "^1.90.0",
    "unplugin-auto-import": "^21.0.0",
    "unplugin-vue-components": "^31.0.0",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^3.6.3",
    "vue": "^3.3.8",
    "vue-tsc": "^2.0.0",
    "vxe-pc-ui": "~4.8.15",
    "vxe-table": "^4.15.6",
    "xe-utils": "^3.7.8"
