{
  "name": "@bingwu/iip-ui-components",
  "version": "1.2.33",
  "description": "iip-ui-vue3 核心组件库",
  "type": "module",
  "main": "./dist/index.es.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "types": "./dist/index.d.ts"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite build --watch",
    "dev:ui": "vite --config vite.dev.config.ts",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:verbose": "vitest --reporter=verbose",
    "test:coverage": "vitest --coverage",
    "test:run": "vitest run",
    "test:run:coverage": "vitest run --coverage",
    "test:run:verbose": "vitest run --reporter=verbose",
    "clean": "rimraf dist dev-dist",
    "prepublishOnly": "pnpm build"
