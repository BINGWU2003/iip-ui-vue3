    "vxe-table": "^4.15.6",
    "xe-utils": "^3.7.8"
  },
  "peerDependencies": {
    "@element-plus/icons-vue": "^2.1.0",
    "element-plus": "^2.4.4",
    "vue": "^3.3.0"
  },
  "devDependencies": {
    "@element-plus/icons-vue": "^2.1.0",
    "@types/node": "^20.9.0",
    "@vitejs/plugin-vue": "^4.5.0",
    "@vue/test-utils": "^2.4.2",
    "element-plus": "^2.4.4",
    "jsdom": "^23.0.1",
    "sass": "^1.90.0",
    "unplugin-auto-import": "^0.17.2",
    "unplugin-vue-components": "^0.25.2",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^3.6.3",
    "vitest": "^0.34.6",
    "vue-tsc": "^2.0.0"
  },
  "keywords": [
    "vue3",
    "typescript",
    "element-plus",
    "components",
    "ui",
    "vue-components",
    "frontend",
    "theme",
    "enterprise"
