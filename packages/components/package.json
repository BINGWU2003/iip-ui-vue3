    "dev:build": "vite build --config vite.dev.config.ts",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "clean": "rm -rf dist dev-dist"
  },
  "dependencies": {
    "@bingwu/iip-ui-utils": "^1.2.7",
    "dayjs": "^1.11.13",
    "vue": "^3.3.8",
